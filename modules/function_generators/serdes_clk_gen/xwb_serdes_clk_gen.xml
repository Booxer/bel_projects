<wbdevice author="Theodor Stana">
    <clockdomain name="sys"></clockdomain>
    <clockdomain name="ref"></clockdomain>
    <generics name="g_num_outputs"></generics>

    <slaveinterface name="wbs" data="32" pages="g_num_outputs">
        <sdb vendorID="GSI" productID="0xc3961dd6" version="1" date="auto" name="xwb_serdes_clk_gen"></sdb>
        <registers>
            <reg name="selr" read="yes" write="yes" selector="yes"
                comment="Selects which channel to control."></reg>
            <reg name="hperr" read="yes" write="yes" paged="yes" clock="ref"
                comment="Half-period register"></reg>
            <reg name="maskr" read="yes" write="yes" paged="yes" clock="ref"
                comment="Bit flip mask register"></reg>
        </registers>
    </slaveinterface>
</wbdevice>

